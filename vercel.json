{
  "version": 3,
  "builds": [
    {
      "src": "api/servidor.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/servidor.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
